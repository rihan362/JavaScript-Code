#code 57

function sumproof(arr)
{
    let sum=0;
    let product=1;
    
    for(let val of arr)
    {
        sum+=val*val;
        product*=val;
        
    }
    console.log(product);
    console.log(sum)
}
sumproof([1,2,3,4])


