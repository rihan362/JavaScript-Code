code 79---------------------------------------

function nthlargest(arr, n) {
  let sorted = [...arr].sort((a, b) => b - a);
  return sorted[n - 1];
}

console.log(nthlargest([43, 56, 23, 89, 88, 90, 99, 652], 4)); // 89



