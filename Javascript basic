#code 48


function happynumr(n)
{
    let happynumber=[];
    let current=1;
    while(happnumber.length<n)
    {
        if(isHappy(current))
        {
            happynumber.push(current);
        }
        current++;
    }
    
    return happynumber;
}


function isHappy(num)
{
    let seen =new set();
    while(num!=1 && !seen.has(num))
    {
        seen.add(num);
        num=getsumsquare(num);
        
    }
    
    return num==1;
    
}

function getsumsquare(num)
{
    let sum=0;
    while(num>0)
    {
        let digit=num%10;
        sum+=digit*digit;
        num=Math.floor(num/10);
    }
    
    return sum;
}



