code 82-----------------------  

function moveElement(arr, fromIndex, toIndex) {
  // Check bounds
  if (fromIndex < 0 || fromIndex >= arr.length || toIndex < 0 || toIndex >= arr.length) {
    console.error("Invalid index");
    return arr;
  }

  const element = arr.splice(fromIndex, 1)[0];
  arr.splice(toIndex, 0, element);             
  return arr;
}


