#code 51

function first(arr, n) {
  if (!Array.isArray(arr) || arr.length === 0) return [];

  if (n == null) {
    return arr[0];
  }

  if (n < 0) {
    return [];
  }

  return arr.slice(0, n);
}

// Test Data
console.log(first([7, 9, 0, -2]));       // 7
console.log(first([], 3));              // []



