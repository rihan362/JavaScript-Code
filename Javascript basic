 code 7

function convertCtoF(c) {
  return (c * 9/5) + 32;
}

function convertFtoC(f) {
  return (f - 32) * 5/9;
}

console.log("60°C is", convertCtoF(60), "°F");
console.log("45°F is", convertFtoC(45), "°C");


