-------------------------------------------
function short_Days(date) {
  return date.toLocaleString('en-us', { weekday: 'short' });
}

code 107----------------------------------

function long_Days(date) {
  return date.toLocaleString('en-us', { weekday: 'long' });
}

code 108------------------------------------

function ISO_numeric_date(date) {
  const day = date.getDay();
  return day === 0 ? 7 : day;
}
























