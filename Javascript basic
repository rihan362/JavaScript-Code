function remove(arr,target)
{
    let result=[];
    for(let i=0;i<arr.length;i++)
    {
        if(arr[i]==target)
        {
            continue;
        }
        result.push(arr[i]);
    }
    return result;
}

console.log(remove([13,3,4],3))


